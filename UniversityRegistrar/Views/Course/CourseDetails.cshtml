@{
}
<h1>Course Details</h1>
    <div class="row">
        <div class="col-sm-4">
            <h3>Course Name: @Model["course"].Name</h3>
        </div>
        <div class="col-sm-4">
            <p>Course Registration #: @Model["course"].CRN</p>
        </div>
        <div class="col-sm-4">
            @foreach(var student in Model["students"])
            {
                <p> @student.Name </p>
                <p> @student.DOE </p>
            }
        </div>
    </div>