@{
}

<h1>Student Details</h1>
    <div class="row">
        <div class="col-sm-4">
            <h3>Name: @Model["student"].Name</h3>
        </div>
        <div class="col-sm-4">
            <p>Date Of Enrollment: @Model["student"].DOE</p>
        </div>
        <div class="col-sm-4">
            @foreach (var course in Model["courses"])
            {
                <p>@course.Name</p>
            }
        </div>
    </div>